    "babel-loader": "^8.0.4",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "style-loader": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-watch": "^4.0.2",
    "npm-run-all": "^4.0.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {    
    "olive-spa": "^3.0.2"
  }
}
